<h1>all bookings</h1>
<%#= raise %>
<% @bookings.each do |booking| %>
  <p><%= booking.id %> </p>
  <p><%= booking.teacher.user.first_name %> <%= booking.teacher.user.last_name %> </p>
  <p><%= booking.student_id %> <%= booking.student.user.first_name %> </p>
<% end %>
